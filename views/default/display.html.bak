{{extend 'layout.html'}}
<h1>
  YOUR INTEREST
</h1>
<table>
<th>{{=kick.name  }} - ${{=kick.price}}</th>
<tr column-gap:40px>
    <td><img src="{{=URL('default','download',args=kick.image)}}" height="200px"/> </td>
    <td>{{=kick.description}}</td>
    <td><span id="{{='item%s'%kick.id}}"></span> {{=A('Add to Cart',callback=URL('cart_callback',vars=dict(id=kick.id,action='add')),target='item%s'%kick.id)}}</td>
</tr>
{{pass}}
</table>
