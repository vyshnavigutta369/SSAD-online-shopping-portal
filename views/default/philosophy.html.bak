{{extend 'layout.html'}}
<h1>PHILOSOPHY</h1>
<table>
{{for p in philosophy:}}
<th>{{=p.name  }} - ${{=p.price}}</th>
<tr column-gap:40px>
    <td><img src="{{=URL('default','download',args=p.image)}}" height="200px"/> </td>
    <td>Author:{{=p.mini}}</td>
    <td>{{=p.description}}</td>
</tr>
{{pass}}
</table>
